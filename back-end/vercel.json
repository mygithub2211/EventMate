{ 
  "version": 2,
  "builds": [
    {
      "src": "*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/",
      "methods" : ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTION"],
      "header" : {
        "Access-Control-Allow-Origin" : "*"
      }
    }
  ]
}
